{
   "name": "bauhaus/config",
   "description": "This package implements a simple config container.",

   "type": "library",
   "license": "MIT",

   "keywords": [
      "bauhaus",
      "config"
   ],

   "authors": [
      {
         "name": "Felipe Martins",
         "homepage": "http://fefas.net"
      }
   ],

   "support": {
      "issues": "https://github.com/bauhausphp/package-config/issues",
      "source": "https://github.com/bauhausphp/package-config"
   },

   "require": {
      "php": ">=7.0",
      "bauhaus/container": "~v1.0"
   },

   "autoload": {
      "psr-4": {
         "Bauhaus\\": "src/"
      }
   },

   "require-dev": {
      "behat/behat": "~3.0",
      "phpunit/phpunit": "5.3.*"
   },

   "autoload-dev": {
      "psr-4": {
         "Bauhaus\\": [
            "tests/acceptance/",
            "tests/unit/"
         ]
      }
   }
}
